<?xml version="1.0" encoding="UTF-8"?>
<DuplicateRule xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <actionOnInsert>Block</actionOnInsert>
    <actionOnUpdate>Block</actionOnUpdate>
    <alertText>There is an existing active Account Plan for this account. Please refer to that Account Plan or clone that Account Plan.</alertText>
    <description>LEM-15222: It is used to maintain the uniqueness of Account Plans.</description>
    <duplicateRuleMatchRules>
        <matchRuleSObjectType>Account_Plan__c</matchRuleSObjectType>
        <matchingRule>Account_Plan_Uniqueness_Matching_Rule</matchingRule>
        <objectMapping xsi:nil="true"/>
    </duplicateRuleMatchRules>
    <isActive>true</isActive>
    <masterLabel>Account Plan Uniqueness Duplicate Rule</masterLabel>
    <securityOption>EnforceSharingRules</securityOption>
    <sortOrder>1</sortOrder>
</DuplicateRule>
