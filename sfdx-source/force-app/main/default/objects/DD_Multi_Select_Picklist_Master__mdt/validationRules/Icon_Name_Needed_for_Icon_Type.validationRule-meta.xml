<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Icon_Name_Needed_for_Icon_Type</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  ISBLANK(   Icon_Name__c ),
  NOT(ISBLANK(TEXT(  Icon_Type__c  )))
)</errorConditionFormula>
    <errorDisplayField>Icon_Name__c</errorDisplayField>
    <errorMessage>Required if Icon Type is specified</errorMessage>
</ValidationRule>
