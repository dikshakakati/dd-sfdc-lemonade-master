<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Results_Width_Needed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  ISPICKVAL( Results_Variant__c , &apos;Dropdown&apos;),
  ISBLANK( Results_Width__c )
)</errorConditionFormula>
    <errorDisplayField>Results_Width__c</errorDisplayField>
    <errorMessage>Required when Results Variant is &apos;Dropdown&apos;</errorMessage>
</ValidationRule>
