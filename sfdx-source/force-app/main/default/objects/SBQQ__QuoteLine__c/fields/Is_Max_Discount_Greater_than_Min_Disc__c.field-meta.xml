<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Max_Discount_Greater_than_Min_Disc__c</fullName>
    <description>LEM-13964: It indicates whether the difference between Max Discount and Min Discount is greater than or equal to 1.</description>
    <externalId>false</externalId>
    <formula>IF(
  (Max_Discount_Amount_for_Consumer__c - (Discount_Percentage_for_Consumer__c * Order_Cart_Minimum__c)) &gt;= 1,
  True,
  False
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Max Discount Greater than Min Disc</label>
    <securityClassification>Proprietary</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
