<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DomainRequiresAnImplementation</fullName>
    <active>true</active>
    <errorConditionFormula>TEXT( ApplicationType__c ) = &apos;Domain&apos; &amp;&amp; ISBLANK( ImplementationType__c )</errorConditionFormula>
    <errorDisplayField>ImplementationType__c</errorDisplayField>
    <errorMessage>Domain bindings must include an Implementation Type.</errorMessage>
</ValidationRule>
