<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Closed__c</fullName>
    <description>LEM-8466 - Calculated boolean to indicate the Status__c is not in an open status. Similar to IsClosed on Opportunity and Case.</description>
    <externalId>false</externalId>
    <formula>OR(ISPICKVAL(Status__c, &quot;Aborted&quot;), ISPICKVAL(Status__c, &quot;Completed&quot;))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Closed</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
