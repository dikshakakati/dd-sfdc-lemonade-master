<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MultiSelect_Ref_is_required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
  ISPICKVAL(Type__c,&apos;MultiSelect Picklist&apos;),
  ISBLANK( MultiSelect_Ref__c )
)</errorConditionFormula>
    <errorDisplayField>MultiSelect_Ref__c</errorDisplayField>
    <errorMessage>This field is required for the type selected.</errorMessage>
</ValidationRule>
