<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>LEM-21068: It provides 'Edit' access on the following Account fields in order to enable MxS persona's users with access to resolve the readiness check failures:
    - Hours of Operation
    - Legal Business Name
    - Tax ID
    - Storefront Website</description>
    <fieldPermissions>
        <editable>true</editable>
        <field>Account.Hours_of_Operation__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Account.Legal_Business_Name__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Account.TaxID__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Account.Storefront_Website__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Merchant Services - Readiness Checks Resolution Special Access</label>
</PermissionSet>
